<Style name="postalcodeboundaries" filter-mode="first">  <Rule>    <Filter>([anzahl] &gt; 5)</Filter>    <LineSymbolizer stroke="#00ff00" stroke-opacity="1" />    <PolygonSymbolizer fill="#00ff00" fill-opacity="1" />  </Rule>  <Rule>    <PolygonSymbolizer fill="#ffffff" fill-opacity="1" />    <LineSymbolizer stroke="#00ff00" stroke-opacity="1" />  </Rule></Style><Layer name="postalcodeboundaries" srs="+proj=merc +a=6378137 +b=6378137 #lat_ts=0.0 #lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">  <StyleName>postalcodeboundaries</StyleName>  <Datasource>    <Parameter name="type"><![CDATA[postgis]]></Parameter>    <Parameter name="table"><![CDATA[(select postal_code, st_simplify( way,200) as way, count(*) as anzahl from planet_osm_polygon left outer join hoteldb.hotel on (postal_code = zip_code) where boundary='postal_code' group by way, postal_code) AS postalcodeboundaries]]></Parameter>    <Parameter name="key_field"><![CDATA[]]></Parameter>    <Parameter name="geometry_field"><![CDATA[]]></Parameter>    <Parameter name="extent_cache"><![CDATA[auto]]></Parameter>    <Parameter name="extent">      <![CDATA[824747.77,7037297.17,1309455.5,7384713.38]]></Parameter>    <Parameter name="dbname"><![CDATA[osm_sh]]></Parameter>    <Parameter name="host"><![CDATA[10.0.2.2]]></Parameter>    <Parameter name="port"><![CDATA[5432]]></Parameter>    <Parameter name="user"><![CDATA[postgres]]></Parameter>    <Parameter name="password"><![CDATA[password]]></Parameter>  </Datasource></Layer>